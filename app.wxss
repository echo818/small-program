/**app.wxss**/
page {
  background-color: #f5f5f5;
  font-size: 28rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fl {
  float: left;
}
.fr {
  float: right;
}
.clearfix {
  zoom:1;
}
.clearfix:after {
  clear:both;
  content:"";
  display:block;
  width: 0;
  height: 0;
  visibility:hidden;
} 
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}